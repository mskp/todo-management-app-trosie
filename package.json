{
  "name": "todo-manager",
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.6",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:app": "pnpm --filter server test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  }
}
